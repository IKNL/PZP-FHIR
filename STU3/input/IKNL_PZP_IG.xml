    disable the display of mappings
    <parameter>
      <code value="excludexml"/>
      <value value="true"/>
    </parameter>
    <parameter>
      <code value="excludejson"/>
      <value value="true"/>
    </parameter>
    <parameter>
      <code value="excludettl"/>
