    </parameter>
    <parameter>
      <code value="find-other-resources" />
      <value value="true" />
    </parameter>
    <parameter>
      <code value="path-liquid" />
      <value value="templates\liquid" />
    </parameter>
    <parameter>
      <code value="shownav" />
      <value value="true" />
    </parameter>
